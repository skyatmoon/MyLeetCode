class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        for i in nums:
            t = target - i
            m = nums.index(i)
            nums[m] = 0.1
            if t in nums:
                return [m,nums.index(t)]
                
